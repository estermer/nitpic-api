<%= form_for(@photo, multipart: true) do |f| %>
  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :photo %>
    <%= f.file_field :photo %>
  </div>
  <div class="actions">
    <%= f.submit 'Upload' %>
    <%= link_to 'Nevermind', photos_path, class: 'button' %>
  </div>
<% end %>
